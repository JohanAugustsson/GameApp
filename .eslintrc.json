{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser":true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
      "linebreak-style": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
      "react/prop-types": 0,
      "prefer-promise-reject-errors":0,
      "no-console": 1,
      "quotes": [2, "single", "avoid-escape"],
      "prefer-destructuring": ["error", {
     "array": true,
     "object": true
     }, {
       "enforceForRenamedProperties": false
     }]
    }
}
